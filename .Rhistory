# Uso de eval y parse_expr:
gather_mod <- function(data,var){
# Armo el vector de nombres:
var_2 <- var %>%
str_c("-",.) %>%
str_c(collapse = ",")
# Defino a "variable" y "valor" como fijas para [1] y [2]
str_c(
'gather(data,"variable","valor",',
var_2,
')'
) %>%
rlang::parse_expr(.) %>%
eval                      #<< [**]
}
